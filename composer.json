{
  "name": "wdmg/yii2-amp",
  "description": "Google AMP pages generator",
  "keywords": ["yii2", "yii2-amp", "google", "amp", "generator", "wdmg"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.0.4",
  "homepage": "https://github.com/wdmg/yii2-amp",
  "support": {
    "source": "https://github.com/wdmg/yii2-amp",
    "issues": "https://github.com/wdmg/yii2-amp/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.40",
    "wdmg/yii2-base": "^1.2.4"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\amp\\": ""
    }
  },
  "extra": {
    "options": {
      "routePrefix": "admin",
      "supportModels": {
        "pages": "wdmg\\pages\\models\\Pages",
        "news": "wdmg\\news\\models\\News",
        "blog": "wdmg\\blog\\models\\Posts"
      },
      "cacheExpire": 3600,
      "ampRoute": "/amp"
    }
  }
}